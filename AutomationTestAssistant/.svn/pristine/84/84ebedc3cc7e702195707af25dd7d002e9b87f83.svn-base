<mui:ModernWindow x:Class="AutomationTestAssistantDesktopApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        xmlns:app="clr-namespace:AutomationTestAssistantDesktopApp"
        Title="MainWindow" Width="Auto" Height="Auto" SizeToContent="WidthAndHeight"  ContentSource="LoginView.xaml">
    <mui:ModernWindow.TitleLinks>
        <mui:Link DisplayName="Settings" Source="/AppereanceSettings.xaml" />
    </mui:ModernWindow.TitleLinks>
    <mui:ModernWindow.MenuLinkGroups>
        <mui:LinkGroup DisplayName="Automation Test Assistant" >
            <mui:LinkGroup.Links>
                <mui:Link DisplayName="Login" Source="/LoginView.xaml" />
                <mui:Link DisplayName="Registration" Source="/RegistrationView.xaml"/>
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
    </mui:ModernWindow.MenuLinkGroups>
</mui:ModernWindow>
